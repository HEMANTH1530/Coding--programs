#include <stdio.h>

int main() {
   int n;
   scanf("%d",&n);
   int arr[n];
   for(int i=0;i<n;i++)
   {
       scanf("%d",&arr[i]);
   }
   int k;
   scanf("%d",&k);
   int maxSum=0,windowSum=0;
   for(int i=0;i<k;i++)
   {
       windowSum+=arr[i];
       
   }
    maxSum=windowSum;
   for(int i=k;i<n;i++)
   {
       windowSum+=arr[i]-arr[i-k];
       if(maxSum<windowSum)
       {
           maxSum=windowSum;
       }
   }
   printf("%d",maxSum);

    return 0;
}
