#include <stdio.h>

int main() {
   int n;
   scanf("%d",&n);
   for(int row=1;row<=n;row++)
   {
       for(int column=1;column<=n;column++)
       {
           if(row==1||row==n||column==1||column==n)
           {
               printf("*");
           }
           else
           {
               printf(" ");
           }
       }
       printf("\n");
   }

    return 0;
}
